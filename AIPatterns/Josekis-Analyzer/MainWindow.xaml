<Window x:Class="Josekis_Analyzer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Josekis_Analyzer"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="300">
    <Grid>
        <StackPanel>
            <Label>Game 1:</Label>
            <Button Name="Game1Path" Click="Game1Path_Click">Browse</Button>
            <Label>Game 2:</Label>
            <Button Name="Game2Path" Click="Game2Path_Click">Browse</Button>
            <TextBlock Name="Result"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
